<!-- Key names are defined here: https://wiki.libsdl.org/SDL_Keycode -->
<keys>
	<key name="Left"   key="Left"/>
	<key name="Right"  key="Right"/>
	<key name="Up"     key="Up"/>
	<key name="Down"   key="Down"/>

	<key name="A"      key="Return"/>
	<key name="B"      key="Backspace"/>

	<key name="Start"  key="Escape"/>
	<key name="Select" key="Tab"/>
</keys>
